{
  "name": "example-start",
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite dev --port 3002",
    "build": "vite build && find node_modules/@prisma/client/runtime/ -type f -name '*.postgresql.wasm' -exec cp '{}' .output/server/node_modules/@prisma/client/runtime/ ';'",
    "start": "PORT=3002 node --env-file node_modules/database/prisma/.env .output/server/index.mjs",
    "clean": "rm -rf .output .vinxi"
  },
  "dependencies": {
    "@premieroctet/next-admin": "workspace:*",
    "@prisma/client": "catalog:prisma",
    "@tanstack/react-router": "catalog:tanstack-start",
    "@tanstack/react-router-devtools": "catalog:tanstack-start",
    "@tanstack/react-start": "catalog:tanstack-start",
    "@tremor/react": "^3.2.2",
    "database": "workspace:*",
    "examples-common": "workspace:*",
    "react": "catalog:react19",
    "react-datepicker": "catalog:",
    "react-dom": "catalog:react19"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "catalog:tailwind",
    "@tailwindcss/vite": "catalog:tailwind",
    "@types/react": "catalog:react19",
    "@types/react-dom": "catalog:react19",
    "@vitejs/plugin-react": "^4.6.0",
    "tailwindcss": "catalog:tailwind",
    "typescript": "^5.8.3",
    "vite": "^6.0.0",
    "vite-tsconfig-paths": "^5.1.4"
  }
}
